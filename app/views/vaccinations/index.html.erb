<h1>Listing Vaccinations</h1>
<%= link_to(image_tag("vac.jpg"), root_path) %>
<table id="Vaccination List">
  <tr>
    <th>Vaccine</th>
    <th>Visit</th>
    <th>Dosage</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @vaccinations.each do |vaccination| %>
  <tr>
    <td><%= vaccination.vaccine_id %></td>
    <td><%= vaccination.visit_id %></td>
    <td><%= vaccination.dosage %></td>
    <td><%= link_to 'Show', vaccination %></td>
    <td><%= link_to 'Edit', edit_vaccination_path(vaccination) %></td>
    <td><%= link_to 'Destroy', vaccination, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'Add New Vaccination', new_vaccination_path %>
